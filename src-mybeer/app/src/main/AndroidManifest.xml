<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_mybeer"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="app.activities.launcher.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="app.activities.main.Main" />
        <activity android:name="app.activities.beer.InformationBeer" />
        <activity android:name="app.activities.beer.AddBeer" />
        <activity android:name="app.activities.brewery.InformationBrewery" />
        <activity android:name="app.activities.brewery.AddBrewery" />
        <activity android:name="app.activities.list.ListBeers" />
        <activity android:name="app.activities.list.ListBreweries" />
    </application>
</manifest>